<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="1500.0" maxWidth="1500.0" minHeight="700.0" minWidth="1080.0" prefHeight="700.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userInterface.ProjectOverviewController">
   <children>
      <Button fx:id="showScheduleBtn" layoutX="220.0" layoutY="136.0" mnemonicParsing="false" onAction="#showScheduleBtnClicked" prefHeight="100.0" prefWidth="300.0" text="Schedule">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button id="showMembersButton" fx:id="showMembersBtn" layoutX="220.0" layoutY="262.0" mnemonicParsing="false" onAction="#showMembersBtnClicked" prefHeight="100.0" prefWidth="300.0" text="Members">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button fx:id="optionsBtn" layoutX="220.0" layoutY="389.0" mnemonicParsing="false" onAction="#optionsBtnClicked" prefHeight="100.0" prefWidth="300.0" text="Edit Project">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button fx:id="dashboard" layoutX="540.0" layoutY="136.0" mnemonicParsing="false" onAction="#getDashboard" prefHeight="100.0" prefWidth="300.0" text="Dashboard">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Label layoutX="318.0" layoutY="64.0" prefHeight="53.0" prefWidth="444.0" text="-Project Overview Menu-">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="backBtn" layoutX="540.0" layoutY="511.0" mnemonicParsing="false" onAction="#backBtnClicked" prefHeight="100.0" prefWidth="300.0" text="Exit" underline="true">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font></Button>
      <Button fx:id="saveProjectBtn" layoutX="540.0" layoutY="389.0" mnemonicParsing="false" onAction="#saveProject" prefHeight="100.0" prefWidth="300.0" text="Save Project">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button fx:id="timeRegister" layoutX="540.0" layoutY="262.0" mnemonicParsing="false" onAction="#timeRegisterClicked" prefHeight="100.0" prefWidth="300.0" text="Register Time">
         <font>
            <Font size="24.0" />
         </font></Button>
      <MenuBar prefHeight="32.0" prefWidth="1080.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
